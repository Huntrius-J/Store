<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_news">

    <fragment
        android:id="@+id/navigation_news"
        android:name="com.example.store.ui.home.HomeFragment"
        android:label="@string/title_news"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_prods"
        android:name="com.example.store.ui.dashboard.ProductsFragment"
        android:label="@string/title_prods"
        tools:layout="@layout/fragment_prods" />

    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.example.store.ui.notifications.CartFragment"
        android:label="@string/title_buys"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/navigation_temp"
        android:name="com.example.store.CartTemp"
        android:label="@string/title_cart"
        tools:layout="@layout/fragment_cart_temp" />
</navigation>